{
  "source": "./src",
  "destination": "./esdoc",
  "package": "./package.json",
  "index": "./README.md",
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "brand": {
          "title": "Cascade",
          "description": "Cryptographic Library",
          "repository": "https://github.com/junkurihara/cascade"
        },
        "lint": {"enable": true},
        "coverage": {"enable": true},
        "test": {
          "source": "./test/",
          "interfaces": ["describe", "it"],
          "includes": ["spec\\.js$"]
        }
      }
    }
  ]
}